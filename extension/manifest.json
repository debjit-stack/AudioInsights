{
  "manifest_version": 3,
  "name": "AudioInsights",
  "version": "1.0",
  "description": "Captures and analyzes audio from any tab.",
  "permissions": [
    "activeTab",
    "tabCapture"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}